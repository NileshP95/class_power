    "class py_pow:\n",
    "    def __init__(self,power,base):\n",
    "        self.power=power\n",
    "        self.base=base\n",
    "    def num(self):\n",
    "        return self.power**self.base\n",
    "powers = py_pow(10,2)\n",
    "print(powers.num())"
